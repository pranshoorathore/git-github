    <main>
     
   <div class="image-slider-container">
    <!-- Card 1 -->
    <div class="card">
        <div class="image-slider">
          
            <img src="./images/Conrad1.jpg" alt="Image 1" class="active">
            <img src="./images/conrad2.jpg" alt="Image 2">
            <img src="./images/conrad3.jpg" alt="Image 3">
            <img src="./images/conrad4.jpg" alt="Image 4">
            <img src="./images/conrad5.jpg" alt="Image 5">


            <button class="prev" onclick="prevSlide(this)">❮</button>
            <button class="next" onclick="nextSlide(this)">❯</button>
        </div>
        <div class="details">
            <h3>Luxury service by CONRAD</h3>
            <p>Hosted by Luies</p>
            <p>₹ 11,750</p>
        </div>
    </div>

    <!-- Card 2 -->
    <div class="card">
        <div class="image-slider">
            <img src="./images/hyatmumbai1.jpg" alt="Image 1" class="active">
            <img src="./images/hyatmumbai2.jpg" alt="Image 2">
            <img src="./images/hyatmumbai3.jpg" alt="Image 3">
            <img src="./images/hyatmumbai4.jpg" alt="Image 4">
            <img src="./images/hyatmumbai5.jpg" alt="Image 5">


            <button class="prev" onclick="prevSlide(this)">❮</button>
            <button class="next" onclick="nextSlide(this)">❯</button>
        </div>
        <div class="details">
            <h3>Make Joyful Memories</h3>
            <p>Hosted by Sarah</p>
            <p>₹ 15,000</p>
        </div>
    </div>

    <!-- Card 3  -->
    <div class="card">
      <div class="image-slider">
        
          <img src="./images/jw-marriot1.jpg" alt="Image 1" class="active">
          <img src="./images/jw-marriot2.jpg" alt="Image 2">
          <img src="./images/jw-marriot3.jpg" alt="Image 3">
          <img src="./images/jw-marriot4.jpg" alt="Image 4">
          <img src="./images/jw-marriot5.jpg" alt="Image 5">


          <button class="prev" onclick="prevSlide(this)">❮</button>
          <button class="next" onclick="nextSlide(this)">❯</button>
      </div>
      <div class="details">
          <h3>Escape, Relax at Our City Oasis</h3>
          <p>Hosted by Jack</p>
          <p>₹ 10,300</p>
      </div>
  </div>
  <!-- Card 4th -->
  <div class="card">
    <div class="image-slider">
      
        <img src="./images/taj1.jpg" alt="Image 1" class="active">
        <img src="./images/taj2.jpg" alt="Image 2">
        <img src="./images/taj3.jpg" alt="Image 3">
        <img src="./images/taj4.jpg" alt="Image 4">
        <img src="./images/taj5.jpeg" alt="Image 5">


        <button class="prev" onclick="prevSlide(this)">❮</button>
        <button class="next" onclick="nextSlide(this)">❯</button>
    </div>
    <div class="details">
        <h3>Our Legendary Flagshiop</h3>
        <p>Hosted by Luies</p>
        <p>₹ 30,000</p>
    </div>
</div>
</div>

  
    
