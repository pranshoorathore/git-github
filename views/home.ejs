    <title>Airbnb Website</title>
    <link rel="stylesheet" href="css/style.css">
</head>
<body>
    <header class="navbar">
        <div class="logo">
            <img src="./images/Airbnb-Logo.jpg" alt="Airbnb Logo">
        </div>
        <div class="search-section">
            <!-- Links section: Stays and Experiences -->
            <nav class="main-nav">
                <ul>
                    <li><a href="#">Stays</a></li>
                    <li><a href="#">Experiences</a></li>
                </ul>
            </nav>
            <!-- Search Bar (Horizontal) -->
            <div class="search-bar-horizontal">
                <div class="search-item">
                    <label for="search">where</label>
                    <input type="text" placeholder="Search Destination" class="search-field">
                </div>
                <div class="search-item">
                    <label for="cin">check in</label>
                    <input type="text" placeholder="Add dates" class="search-field">
                </div>
                <div class="search-item">
                    <label for="cout">check out</label>
                    <input type="text" placeholder="Add dates" class="search-field">
                </div>
                <div class="search-item">
                    <label for="who">who</label>
                    <input type="text" placeholder="Add guests" class="search-field">
                </div>
                <button class="search-btn">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
